[
  {
    "command": ["ls", "-la"],
    "image": "${container_image}",
    "fargatePlatformConfiguration": {
      "platformVersion" : "LATEST"
    },

    "resourceRequirements": [
      {
        "type": "VCPU",
        "value":"${container_vcpu}"
      },
      {
        "type": "MEMORY",
        "value" :"${container_memory}"
      }
    ],
    "executionRoleArn": "${executionRoleArn}"
  }
]